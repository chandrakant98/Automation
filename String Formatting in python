my_name = 'Sanvi Gupta'
my_age = 10
my_height = 61
my_weight = 60
my_eyes = 'Black'
my_teeth = 'White'
my_hair = 'Brown'

print("Let's talk about %s. " % my_name)
print("Her's %d inches tall." % my_height)
print("Her's %d killo heavy." % my_weight)
print("Actually Sanvi is not too heavy.")
print("Her's got %s eyes and %s hair."% (my_eyes, my_hair))
print("Her teeth are usually %s depending on the chocolate." % my_teeth)

print("If I add %d, %d, and %d I get %d." % (my_age, my_height, my_weight, my_age + my_height + my_weight))
